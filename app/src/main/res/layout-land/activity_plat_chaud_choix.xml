<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="168dp"
        android:layout_height="149dp"
        android:contentDescription="Main Meal"
        app:layout_constraintBottom_toTopOf="@+id/MainCourseCheck"
        app:layout_constraintEnd_toStartOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/MealChoiceHot"
        app:layout_constraintVertical_bias="0.883"
        app:srcCompat="@drawable/_978167"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="169dp"
        android:layout_height="159dp"
        android:contentDescription="Dessert"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.882"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.376"
        app:srcCompat="@drawable/_3b4ea0e6d4e81812fa81c7d16c2062d"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="168dp"
        android:layout_height="152dp"
        android:contentDescription="Appetizer"
        app:layout_constraintBottom_toTopOf="@+id/SaladCheck"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.126"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.894"
        app:srcCompat="@drawable/_515183"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/MealChoiceHot"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/MealChoiceHot"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:textAlignment="center"
        android:textSize="27sp"
        android:textStyle="bold"
        android:textColor="@color/bg_txtTitle"/>

    <CheckBox
        android:id="@+id/MainCourseCheck"
        android:layout_width="141dp"
        android:layout_height="69dp"
        android:text="@string/MainCourseCheck"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btnBack4"
        app:layout_constraintEnd_toStartOf="@+id/DessertCheck"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toEndOf="@+id/SaladCheck"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.953" />

    <CheckBox
        android:id="@+id/SaladCheck"
        android:layout_width="144dp"
        android:layout_height="66dp"
        android:layout_marginStart="84dp"
        android:text="@string/SaladCheck"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.771" />

    <CheckBox
        android:id="@+id/DessertCheck"
        android:layout_width="138dp"
        android:layout_height="74dp"
        android:layout_marginEnd="84dp"
        android:text="@string/DessertCheck"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.055" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnBack4"
        style="@style/btnBC"
        android:layout_width="139dp"
        android:layout_height="56dp"
        android:onClick="onClick"
        android:text="@string/btnBack"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.822" />
</androidx.constraintlayout.widget.ConstraintLayout>